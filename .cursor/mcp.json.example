{
  "mcpServers": {
    "db-server-stdio": {
      "command": "/path/to/mcp-server",
      "args": ["-transport", "stdio"],
      "env": {
        "DB_TYPE": "mysql",
        "DB_HOST": "localhost",
        "DB_PORT": "3306",
        "DB_USER": "your_username",
        "DB_PASSWORD": "your_password",
        "DB_NAME": "your_database"
      }
    },
    "db-server-sse": {
      "url": "http://localhost:9090/sse"
    }
  }
} 